  "state_off": 0,
  "dev": {
    "identifiers": [
      "{{ device_id }}"
    ],
    "manufacturer": "Zendure",
    "model": "Solarflow",
    "name": "Solarflow Hub",
    "sw_version": "{{ fw_version }}"
  },
  "~": "solarflow-hub/{{ device_id }}/telemetry/"
}