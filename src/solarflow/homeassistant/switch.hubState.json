  "stat_t": "~hubState",
  "uniq_id": "{{ device_id }}-hubStateswitch",
  "dev_cla": "switch",
  "payload_on": "{\"properties\": {\"hubState\": 1 }}",
  "payload_off": "{\"properties\": {\"hubState\": 0 }}",
